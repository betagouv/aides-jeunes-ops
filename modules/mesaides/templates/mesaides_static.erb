location /favicon.ico {
  rewrite ^.*$ /img/favicon/favicon.ico;
}

location ~ ^/(fonts|js|styles) {
  access_log off;
  expires 1y;
  add_header Cache-Control private;
  try_files /dist$uri =404;
}

location ~ ^/(img|documents) {
  access_log off;
  expires 5m;
  add_header Cache-Control private;
  try_files /dist$uri =404;
}
